<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.decobarri.decobarri">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".project_menu.ProjectMenuActivity"/>
        <activity android:name=".main_menu.MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_resources.CreateProject"/>
        <activity android:name=".Register" />
        <activity android:name=".Login"/>
        <activity android:name=".drawe_menu.AccountSettingsActivity" />
        <activity android:name=".drawe_menu.ChatActivity" />
        <activity android:name=".drawe_menu.ContactListActivity" />
    </application>

</manifest>